{
  "name": "@cyclone-ui/accordion",
  "version": "0.3.0",
  "type": "module",
  "description": "A package containing the Accordion component used by Storm Software",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/cyclone-ui.git",
    "directory": "components/accordion"
  },
  "private": false,
  "main": "dist/cjs",
  "module": "dist/esm",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "react-native": "./dist/cjs/index.native.js"
    }
  },
  "types": "./types/index.d.ts",
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.3"
  },
  "dependencies": {
    "@storm-stack/types": "latest",
    "@tamagui/accordion": "^1.117.2",
    "@tamagui/collapsible": "^1.117.2",
    "@tamagui/core": "^1.117.2",
    "@tamagui/group": "^1.117.2",
    "@tamagui/helpers": "^1.117.2",
    "@tamagui/stacks": "^1.117.2",
    "@tamagui/separator": "^1.117.2"
  },
  "devDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "module:jsx": "dist/jsx",
  "source": "src/index.ts"
}
