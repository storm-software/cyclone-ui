{
  "name": "@cyclone-ui/font-mona-sans",
  "version": "0.1.0",
  "type": "module",
  "description": "A Cyclone UI package containing the Mono-Sans font and font-related utilities used in the design system",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/cyclone-ui.git",
    "directory": "packages/font-mona-sans"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    "./css/200.css": "./css/200.css",
    "./css/500.css": "./css/500.css",
    "./css/700.css": "./css/700.css",
    "./css/800.css": "./css/800.css",
    "./css/900.css": "./css/900.css",
    "./css/style.css": "./css/style.css",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "dependencies": {
    "@tamagui/constants": "^1.98.0",
    "@tamagui/core": "^1.98.0",
    "@tamagui/web": "^1.98.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
