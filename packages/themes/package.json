{
  "name": "@cyclone-ui/themes",
  "version": "0.0.1",
  "private": false,
  "description": "Shared theme configuration files used by Cyclone UI",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/cyclone-ui.git",
    "directory": "packages/themes"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "dist/cjs",
  "module": "dist/esm",
  "types": "./types/index.d.ts",
  "dependencies": {
    "@swc-node/register": "~1.8.0",
    "@swc/core": "~1.3.85",
    "@tamagui/colors": "^1.95.3",
    "@tamagui/create-theme": "^1.95.3",
    "@tamagui/theme-builder": "^1.95.3",
    "@tamagui/web": "^1.95.3"
  },
  "devDependencies": {
    "@tamagui/cli": "^1.95.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "module:jsx": "dist/jsx"
}
