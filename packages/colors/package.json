{
  "name": "@cyclone-ui/colors",
  "version": "0.2.0",
  "type": "module",
  "description": "A repository containing [Tamagui](https://github.com/tamagui/tamagui) based design components used by Storm Software. Like [shadcn/ui](https://github.com/shadcn-ui/ui), the components are copied into other repositories via the Cyclone CLI.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/cyclone-ui.git",
    "directory": "packages/colors"
  },
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./brand": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/brand-themes.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/brand-themes.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/brand-themes.js"
      }
    }
  },
  "dependencies": {
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@tamagui/colors": "^1.98.0",
    "@tamagui/web": "^1.98.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
