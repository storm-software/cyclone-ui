{
  "$schema": "https://cdn.jsdelivr.net/npm/@storm-software/config/schemas/storm.schema.json",
  "extends": "./node_modules/@storm-software/config/presets/base.json",
  "name": "cyclone-ui",
  "namespace": "cyclone-ui",
  "repository": "https://github.com/storm-software/cyclone-ui",
  "organization": "storm-software",
  "ci": false,
  "owner": "cyclone-ui",
  "timezone": "America/New_York",
  "locale": "en-US",
  "logLevel": "all",
  "packageManager": "pnpm",
  "externalPackagePatterns": ["@storm-software/"],
  "colors": {
    "light": {
      "background": "#f4f4f5",
      "foreground": "#121212",
      "brand": "#1fb2a6",
      "help": "#8250df",
      "success": "#087f5b",
      "info": "#0550ae",
      "warning": "#e3b341",
      "error": "#a40e26"
    },
    "dark": {
      "background": "#22272E",
      "foreground": "#F6F8FA",
      "brand": "#1fb2a6",
      "help": "#8256D0",
      "success": "#087f5b",
      "info": "#58a6ff",
      "warning": "#F3D371",
      "error": "#d1242f"
    }
  },
  "extensions": {
    "telemetry": {
      "fileName": "storm",
      "fileExtension": "log",
      "path": "tmp/storm/logs",
      "stacktrace": true
    }
  }
}
