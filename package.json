  "scripts": {
    "adr": "pnpm log4brains adr new",
    "adr-preview": "pnpm log4brains preview",
    "api-extractor": "pnpm storm-docs api-extractor --outputPath=\"docs/api-reference\" --clean",
    "build-all": "pnpm build-plugins && nx affected -t build --parallel=3 --nxBail --exclude=\"components/*,apps/storybook,@cyclone-ui/monorepo\"",
    "build-apps": "nx run-many -t build --projects=apps/* --parallel=3",
    "build-components": "nx run-many -t build --projects=components/* --parallel=3",
    "build-docs": "nx run-many -t build --projects=docs/* --parallel=3",
    "build-packages": "nx run-many -t build --projects=packages/* --parallel=3",
    "build-plugins": "node -r esbuild-register tools/scripts/build-plugins.js",
    "build-prod": "pnpm build-plugins && nx run-many -t build --all --prod --parallel=5 --exclude=\"components/*,apps/storybook,@cyclone-ui/monorepo\"",
    "build-storybook": "storybook build -s public",
    "build-tools": "nx run-many -t build --projects=tools/* --parallel=5",
    "clean": "pnpm exec rimraf dist",
    "clean:apps": "pnpm exec rimraf dist/apps",
    "clean:components": "pnpm exec rimraf dist/components",
    "clean:docs": "pnpm exec rimraf dist/docs",
    "clean:packages": "pnpm exec rimraf dist/packages",
    "clean:storybook": "pnpm exec rimraf dist/storybook",
    "clean:tools": "pnpm exec rimraf dist/tools",
    "commit": "pnpm storm-git commit",
    "dep-graph": "nx dep-graph",
    "e2e": "nx e2e",
    "format": "nx format:write",
    "format-sherif": "pnpx sherif -f -i typescript -i react -i react-dom -i react-native",
    "graph": "nx graph",
    "help": "nx help",
    "install-csb": "corepack enable && pnpm install --no-frozen-lockfile",
    "lighthouse": "npx lighthouse http://localhost:3000 --view --chrome-flags='--headless' --throttling.cpuSlowdownMultiplier=16 --output-path=/tmp/report.html",
    "lighthouse:live": "npx lighthouse https://cyclone-ui.com --view --chrome-flags='--headless' --throttling.cpuSlowdownMultiplier=16 --output-path=/tmp/report.html",
    "lint": "pnpm storm-lint all --skip-circular-deps",
    "lint-knip": "pnpx knip",
    "lint-sherif": "pnpx sherif -i typescript -i react -i react-dom -i react-native",
    "local-registry": "nx local-registry @cyclone-ui/monorepo",
    "nuke": "pnpm nuke:cache && pnpm nuke:node_modules",
    "nuke:cache": "pnpm exec rimraf --no-interactive -- ./dist ./tmp ./pnpm-lock.yaml ./.tamagui",
    "nuke:node_modules": "pnpm nuke:node_modules-apps && pnpm nuke:node_modules-components && pnpm nuke:node_modules-packages && pnpm nuke:node_modules-tools && pnpm exec rimraf --no-interactive --glob ./node_modules",
    "nuke:node_modules-apps": "pnpm exec rimraf --no-interactive --glob ./apps/**/node_modules",
    "nuke:node_modules-components": "pnpm exec rimraf --no-interactive --glob ./components/**/node_modules",
    "nuke:node_modules-packages": "pnpm exec rimraf --no-interactive --glob ./packages/**/node_modules",
    "nuke:node_modules-tools": "pnpm exec rimraf --no-interactive --glob ./tools/**/node_modules",
    "nx": "nx",
    "preinstall": "npx -y only-allow pnpm",
    "push": "git push origin main:main",
    "readme-gen": "pnpm storm-git readme-gen --templates=\"docs/readme-templates\"",
    "release": "pnpm storm-git release",
    "start": "nx serve"
  },
